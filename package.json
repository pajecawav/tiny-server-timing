{
	"name": "tiny-server-timing",
	"version": "0.1.4",
	"repository": "https://github.com/pajecawav/tiny-server-timing.git",
	"bugs": "https://github.com/pajecawav/tiny-server-timing/issues",
	"homepage": "https://github.com/pajecawav/tiny-server-timing#readme",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"files": [
		"dist/*.js",
		"dist/*.mjs",
		"dist/*.js.map",
		"dist/*.mjs.map",
		"dist/*.d.ts"
	],
	"license": "MIT",
	"sideEffects": false,
	"scripts": {
		"build": "tsup",
		"test": "vitest run",
		"test:watch": "vitest watch",
		"test:coverage": "vitest run --coverage",
		"test:ui": "vitest --ui",
		"lint": "run-p -l lint:*",
		"lint:eslint": "eslint .",
		"lint:tsc": "tsc --noEmit",
		"lint:format": "prettier . --check",
		"format": "prettier . --write",
		"prepare": "husky install",
		"prepublishOnly": "run-p -l lint:* test && pnpm build"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.7.2",
		"@commitlint/config-conventional": "^17.7.0",
		"@typescript-eslint/eslint-plugin": "^6.7.4",
		"@typescript-eslint/parser": "^6.7.4",
		"@vitest/coverage-v8": "^0.34.6",
		"@vitest/ui": "^0.34.6",
		"eslint": "^8.51.0",
		"husky": "^8.0.3",
		"lint-staged": "^14.0.1",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.0.3",
		"tsup": "^7.2.0",
		"typescript": "^5.2.2",
		"vitest": "^0.34.6"
	}
}
