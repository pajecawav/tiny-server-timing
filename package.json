{
	"name": "tiny-server-timing",
	"version": "0.1.0",
	"repository": "https://github.com/pajecawav/tiny-server-timing.git",
	"bugs": "https://github.com/pajecawav/tiny-server-timing/issues",
	"homepage": "https://github.com/pajecawav/tiny-server-timing#readme",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"files": [
		"dist/*.js",
		"dist/*.js.map",
		"dist/*.d.ts"
	],
	"license": "MIT",
	"sideEffects": false,
	"scripts": {
		"build": "tsup",
		"test": "vitest run",
		"test:watch": "vitest watch",
		"test:coverage": "vitest run --coverage",
		"test:ui": "vitest --ui",
		"lint": "run-p -l lint:*",
		"lint:eslint": "eslint .",
		"lint:tsc": "tsc --noEmit",
		"lint:format": "prettier . --check",
		"format": "prettier . --write",
		"prepare": "husky install",
		"prepublishOnly": "run-p -l lint:* test && pnpm build"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.4.2",
		"@commitlint/config-conventional": "^17.4.2",
		"@typescript-eslint/eslint-plugin": "^5.51.0",
		"@typescript-eslint/parser": "^5.51.0",
		"@vitest/coverage-c8": "^0.28.4",
		"@vitest/ui": "^0.28.4",
		"eslint": "^8.34.0",
		"husky": "^8.0.3",
		"lint-staged": "^13.1.1",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.8.4",
		"tsup": "^6.6.0",
		"typescript": "^4.9.5",
		"vitest": "^0.28.4"
	}
}
